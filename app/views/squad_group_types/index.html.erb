<p id="notice"><%= notice %></p>

<h1>Squad Group Types</h1>

<table>
  <thead>
    <tr>
      <th>Squadgroup</th>
      <th>Exercise type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @squad_group_types.each do |squad_group_type| %>
      <tr>
        <td><%= squad_group_type.squadgroup %></td>
        <td><%= squad_group_type.exercise_type %></td>
        <td><%= link_to 'Show', squad_group_type %></td>
        <td><%= link_to 'Edit', edit_squad_group_type_path(squad_group_type) %></td>
        <td><%= link_to 'Destroy', squad_group_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Squad Group Type', new_squad_group_type_path %>
